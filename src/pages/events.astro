---
import BaseHead from '@/components/BaseHead.astro'
import Header from '@/components/Header.astro'
import Footer from '@/components/Footer.astro'
import GalleryEvents from '@/components/GalleryEvents.astro'
import ExternalScripts from '@/components/ExternalScripts.astro'

let title = 'REPEAT DANCE PRES BENJAMIN'
let description = 'Photos by @n.timson'
---

<!doctype html>
<html lang="en">
  <BaseHead
    title={title}
    description={description}
    author={'M.Parker'}
    og={'og-image.jpg'}
  />

  <body class="is-preload">
    <Header title={title} />
    <Footer title={title} description={description} />
    <GalleryEvents title={title} description={description} />
    <ExternalScripts />
  </body>
</html>
